{
  "name": "meltdown",
  "version": "0.1.0",
  "description": "A markdown editor",
  "private": true,
  "main": "app/index.tsx",
  "scripts": {
    "postinstall": "npm-run-all postinstall:*",
    "postinstall:copy_base_config": "cp tsconfig.base.json tsconfig.json",
    "postinstall:typings": "tsdm rewire",
    "postinstall:tsconfig_glob": "npm run tsconfig_glob",
    "start": "webpack-dev-server --hot --inline",
    "lint": "tslint `find app -name '*.ts' -o -name '*.tsx'`",
    "tsconfig_glob": "tsconfig -i 2",
    "build": "rm -rf dist && NODE_ENV=production webpack",
    "build_dev": "webpack",
    "typescript": "tsc",
    "test": "npm-run-all test:*",
    "test:unit": "mocha --require ts-node/register app/**/*.spec.ts",
    "test-watch": "npm run test:unit -- --watch --watch-extensions ts, tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pajn/Culinam.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Pajn/Culinam/issues"
  },
  "homepage": "https://github.com/Pajn/Culinam#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-insert-css-module": "^1.0.3",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "mock-functions": "^0.1.1",
    "npm-run-all": "^1.5.1",
    "react-hot-loader": "^1.3.0",
    "retyped-chai-tsd-ambient": "^3.4.0-0",
    "retyped-mocha-tsd-ambient": "^2.2.5-0",
    "retyped-react-redux-tsd-ambient": "^2.1.2-0",
    "retyped-react-router-tsd-ambient": "^1.0.0-0",
    "retyped-react-tsd-ambient": "^0.14.0-1",
    "retyped-redux-tsd-ambient": "^1.0.0-0",
    "ts-loader": "^0.8.1",
    "tsconfig-glob": "^0.4.1",
    "tsdm": "0.1.0-3",
    "tslint": "^3.5.0",
    "typescript": "^1.9.0-dev.20160227",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "commonmark": "^0.24.0",
    "draft-js": "^0.1.0",
    "lodash": "^4.5.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-hyperscript-helpers": "^1.0.1",
    "react-markdown": "^2.0.1",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "recompose": "^0.15.0",
    "redux": "^3.3.1",
    "redux-debounced": "^0.1.1",
    "redux-decorated": "^0.1.3",
    "redux-thunk": "^1.0.3",
    "zenscroll": "^3.0.1"
  }
}
